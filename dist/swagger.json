{
    "openapi": "3.0.3",
    "info": {
        "title": "News Media Server",
        "description": "API for managing news information",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://news-media-server-idn.vercel.app/"
        },
        {
            "url": "http://localhost:3000"
        }
    ],
    "tags": [
        {
            "name": "newsHeadlines",
            "description": "Operations to interact with news headline information"
        }
    ],
    "paths": {
        "/v1/news-headlines": {
            "get": {
                "tags": [
                    "newsHeadlines"
                ],
                "summary": "Get all news headlines information",
                "operationId": "viewNewsHeadlines",
                "responses": {
                    "200": {
                        "description": "Ok"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/v1/news-headline": {
            "post": {
                "tags": [
                    "newsHeadlines"
                ],
                "summary": "Add a news headline information",
                "operationId": "addNewsHeadline",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/addNewsHeadline"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/v1/news-headline/{id}": {
            "put": {
                "tags": [
                    "newsHeadlines"
                ],
                "summary": "Edit news headline information by ID",
                "operationId": "editNewsHeadlineById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the news headline",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/editNewsHeadlineById"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Ok"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            },
            "patch": {
                "tags": [
                    "newsHeadlines"
                ],
                "summary": "Partial edit news headline information by ID",
                "operationId": "partialEditNewsHeadlineById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the news headline",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": false,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/partialEditNewsHeadlineById"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Ok"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            },
            "delete": {
                "tags": [
                    "newsHeadlines"
                ],
                "summary": "Soft remove news headline record by ID",
                "operationId": "softRemoveNewsHeadlineById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the news headline",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No Content"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/v1/news-headline/hard/{id}": {
            "delete": {
                "tags": [
                    "newsHeadlines"
                ],
                "summary": "Remove news headline record by ID",
                "operationId": "removeNewsHeadlinesById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the news headline",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No Content"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "addNewsHeadline": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "body": {
                        "type": "string"
                    },
                    "userId": {
                        "type": "integer"
                    }
                }
            },
            "editNewsHeadlineById": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "body": {
                        "type": "string"
                    }
                }
            },
            "partialEditNewsHeadlineById": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "body": {
                        "type": "string"
                    }
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }
}
